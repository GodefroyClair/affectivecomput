fapply <- function(x, f, ...) {
out <- vector("list", length(x))
for (i in seq_along(x)) {
res <- f(x[i], ...)
stopifnot(is.list(x) && is.vector(x))
out[[i]] <- res
}
unlist(out, recursive = FALSE)
}
